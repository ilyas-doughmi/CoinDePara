var textoIndesejado = "Sua solução de balcão único para imóveis na Geórgia";

// Função para verificar e remover o texto desejado
function verificarTexto() {
  // Encontre todos os elementos com a classe gt-block
  var gtBlocks = document.querySelectorAll('.gt-block');
  
  // Loop através de todos os elementos com a classe gt-block
  gtBlocks.forEach(function(block) {
    // Encontre o elemento strong dentro de cada gt-block
    var strongElement = block.querySelector('strong');
    
    // Verifique se o strongElement existe e se o seu texto contém a frase desejada
    if (strongElement && strongElement.textContent.includes(textoIndesejado)) {
      // Remova o texto desejado do strongElement
      var novoTexto = strongElement.textContent.replace(textoIndesejado, '');
      strongElement.textContent = novoTexto;
    }
  });
}

// Configurar um intervalo para verificar a cada 1 segundo durante 5 segundos
var tempoLimite = 5000; // 5 segundos
var intervalo = 1000; // 1 segundo
var tempoDecorrido = 0;

var buscaInterval = setInterval(function() {
  verificarTexto();
  tempoDecorrido += intervalo;

  // Se o tempo decorrido for igual ou superior ao tempo limite, pare de verificar
  if (tempoDecorrido >= tempoLimite) {
    clearInterval(buscaInterval);
  }
}, intervalo);


